public class char_pramid {
    public static void main(String[] args) {
        char last = 'c' , alphabet = 'C';
        for(int i = 3 ; i <= (last-'A' + 1); i++){
            for(int j = 1; j <= i; j++){
                System.out.print(alphabet + " ");
            }
            alphabet++;
            System.out.println();
        }
    }
}
